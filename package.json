{
  "name": "@koide-labs/ui",
  "version": "0.0.6",
  "type": "module",
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./components/*": {
      "types": "./dist/components/*/index.d.ts",
      "default": "./dist/components/*/index.js"
    },
    "./integrations/tailwind.css": "./dist/integrations/tailwind.css",
    "./integrations/*": {
      "types": "./dist/integrations/*/index.d.ts",
      "default": "./dist/integrations/*/index.js"
    },
    "./styles/core.css": "./dist/styles/core.css",
    "./styles/tokens": {
      "types": "./dist/styles/tokens.d.ts",
      "default": "./dist/styles/tokens.js"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "packageManager": "pnpm@10.19.0",
  "scripts": {
    "dev": "vite",
    "build": "npm-run-all build:icons build:vite",
    "build:icons": "tsx ./scripts/build-icon-types.ts",
    "build:vite": "tsc -b && vite build",
    "preview": "vite preview",
    "prepare": "husky",
    "typecheck": "tsc -b --noEmit",
    "lint": "npm-run-all --parallel lint:*",
    "lint:ts": "eslint .",
    "lint:style": "stylelint src/**/*.module.css",
    "format": "prettier --write .",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "@base-ui/react": "^1.0.0",
    "@radix-ui/react-visually-hidden": "^1.2.4",
    "autosize": "^6.0.1",
    "clsx": "^2.1.1",
    "cmdk": "github:npham-dev/cmdk#main&path:/cmdk",
    "date-fns": "^4.1.0",
    "modern-normalize": "^3.0.1",
    "prismjs": "^1.30.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-markdown": "^10.1.0",
    "remixicon": "^4.8.0"
  },
  "peerDependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "remixicon": "^4.8.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.3",
    "@eslint/js": "^9.39.1",
    "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
    "@storybook/addon-a11y": "^10.1.11",
    "@storybook/addon-docs": "^10.1.11",
    "@storybook/addon-vitest": "^10.1.11",
    "@storybook/builder-vite": "^10.1.11",
    "@storybook/react-vite": "^10.1.11",
    "@types/autosize": "^4.0.3",
    "@types/node": "^24.10.1",
    "@types/prismjs": "^1.26.5",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/browser-playwright": "^4.0.16",
    "@vitest/coverage-v8": "^4.0.16",
    "autoprefixer": "^10.4.23",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "eslint-plugin-storybook": "^10.1.11",
    "glob": "^13.0.0",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "npm-run-all": "^4.1.5",
    "playwright": "^1.57.0",
    "postcss": "^8.5.6",
    "postcss-nested-import": "^1.3.0",
    "postcss-nesting": "^13.0.2",
    "prettier": "^3.7.4",
    "prettier-plugin-css-order": "^2.1.2",
    "storybook": "^10.1.11",
    "stylelint": "^16.26.1",
    "stylelint-config-standard": "^39.0.1",
    "stylelint-selector-bem-pattern": "^4.0.1",
    "tsx": "^4.21.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "typescript-plugin-css-modules": "^5.2.0",
    "vite": "^7.2.4",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-lib-inject-css": "^2.2.2",
    "vite-tsconfig-paths": "^6.0.3",
    "vitest": "^4.0.16"
  }
}
